<?xml version="1.0" encoding="utf-8"?>
<cubson-mapping>
  <class table="cat">
    <id name="cat_id" type="XOBJ_DTYPE_INT" column="cat_id">
      <generator type="increment" />
    </id>
    <property name="cat_title" type="XOBJ_DTYPE_STRING" column="cat_title" not-null="true" length="255" />
    <property name="gr_id" type="XOBJ_DTYPE_INT" column="gr_id" not-null="true" default="0" />
    <property name="p_id" type="XOBJ_DTYPE_INT" column="p_id" not-null="true" default="0" />
    <property name="cat_desc" type="XOBJ_DTYPE_TEXT" column="cat_desc" not-null="true" />
    <property name="weight" type="XOBJ_DTYPE_INT" column="weight" not-null="true" default="0" />
    <property name="actions" type="XOBJ_DTYPE_TEXT" column="actions" not-null="true" />
    <many-to-one name="mGr" column="gr_id" class="Entity.gr" />
  </class>
</cubson-mapping>